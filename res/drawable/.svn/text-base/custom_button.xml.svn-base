<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#ffffeee9"
                android:endColor="#fffeb5ab"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ffb2b2b2" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>

    <item android:state_focused="true" >
        <shape>
            <gradient
                android:startColor="#fff7f9ff"
                android:endColor="#ffccdfff"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ffb2b2b2" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>

    <item>        
        <shape>
            <gradient
                android:startColor="#fff9f9f9"
                android:endColor="#ffd9d9d9"
                android:angle="270"  />
            <stroke
                android:width="1dp"
                android:color="#ffb2b2b2" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>
